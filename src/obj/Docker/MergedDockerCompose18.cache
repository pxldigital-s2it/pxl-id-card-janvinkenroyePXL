services:
  presences.api:
    build:
      context: D:\PXLProjects\DotNET Expert\pxl-id-card-janvinkenroyePXL\src
      dockerfile: Services/Presences/Presences.api/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Presences.Api
      target: base
    container_name: Presences.Api
    depends_on:
      sqldata:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      ConnectionString: Data Source=sqldata;Database=IDCard.Presences;User Id=sa;Password=Pass@Word
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: presencesapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net5.0/Presences.Api.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 5000
      target: 80
    - published: 5001
      target: 443
    tty: true
    volumes:
    - D:\PXLProjects\DotNET Expert\pxl-id-card-janvinkenroyePXL\src\Services\Presences\Presences.api:/app:rw
    - C:\Users\Jan Vinkenroye\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Jan Vinkenroye\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Jan Vinkenroye\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft\Xamarin\NuGet:/root/.nuget/fallbackpackages:ro
    - C:\Users\Jan Vinkenroye\.nuget\packages:/root/.nuget/packages:ro
    - D:\PXLProjects\DotNET Expert\pxl-id-card-janvinkenroyePXL\src:/src:rw
  sqldata:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Pass@Word
    image: mcr.microsoft.com/mssql/server:2019-latest
    ports:
    - published: 1433
      target: 1433
    volumes:
    - pxl-sqldata:/var/opt/mssql:rw
version: '3.4'
volumes:
  pxl-sqldata:
    external: false